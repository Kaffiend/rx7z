{
  "compilerOptions": {
      "version": true,
      "module": "commonjs",
      "target": "es6",
      "sourceMap": true,
      "declaration": true,
      "experimentalDecorators": true,
      "outDir": "dist",
      "moduleResolution": "node",
      "strictNullChecks": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "typeRoots": [
          "node_modules/@types"
      ]

  },
  "exclude": [
      "dist/",
      "node_modules"
  ]
}